@model MyPortfolio.DAL.Entities.Skill

@{
	ViewData["Title"] = "CreateSkill";
	Layout = "~/Views/Layout/Index.cshtml";
}


<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Deneyimler</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">Deneyim Ekleme Sayfası</div>
					</div>
					<div class="card-body">

						<form method="post">

							<label>Yetenek</label>
							<input type="text" asp-for="Title" class="form-control" />
							<br />
							<label>Değer</label>
							<input type="text" asp-for="Value" class="form-control" />
							<br />

							<button class="btn btn-primary">Deneyimi Ekle</button>
						</form>

					</div>
				</div>
			</div>
		</div>
	</div>
</div>




